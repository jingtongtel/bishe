@Component
public class PopulationRegionRangeHandler extends PopulationRegionDefHandler {
	



	@Override
	protected String getDataViewName() {
		// TODO Auto-generated method stub
		return "range";
	}
	
	
	
	@Override
	protected Object formatData(Map paraMap) {
		// TODO Auto-generated method stub
		
		
		//生成range
		int divisor=(int) paraMap.get("range");
		if (divisor==0) {
			 divisor=5;
		}
		
		List<Map> list = (List<Map>) super.formatData(paraMap);
		System.out.println(list.get(0).get("USERSUM"));
		Object ob  =  list.get(0).get("USERSUM");
		int Divide=Integer.parseInt(ob.toString());
		double entropy=(double)Divide/divisor;
		int group= (int) Math.ceil(entropy);
		List<String> rangList = new ArrayList<String>();
		for (int i = 0; i < divisor; i++) {
			String rangS=group*i+"-"+group*(i+1);
			rangList.add(i, rangS);
		}
		
		
		
		//遍历生成rangid
		for (int i = 0; i < list.size()-1; i++) {
			ob =  list.get(i).get("USERSUM");
			Divide=Integer.parseInt(ob.toString());
			System.out.println(Divide);
			System.out.println(entropy);
			System.out.println(Divide/entropy);
			double rangeIdx=(double)Divide/group;
			list.get(i).put("rangeIdx", Math.ceil(rangeIdx));
			
			
		}

		Map finalMap = new HashMap();
		finalMap.put("dataArray", list);
		finalMap.put("range", rangList);
		
		return finalMap;
	}

}
