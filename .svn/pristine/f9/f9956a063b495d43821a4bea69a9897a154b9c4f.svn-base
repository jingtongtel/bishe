package com.boco.mbd.mapper.demo;

import java.util.HashMap;
import java.util.List;

import org.springframework.stereotype.Repository;

import com.boco.mbd.bean.demo.Student;

/**
 * 用于操作Student对象的持久层接口，与myBatis映射文件student.xml对应
 * @author liangpeng
 * @date 2017-02-07
 */
@Repository
public interface StudentMapper {

	/**
	 * 全量查询
	 * @return
	 */
	public List<Student> queryAll();
	/**
	 * 根据条件查询
	 * @param map
	 * @return
	 */
	public List<Student> queryStudent(HashMap map);
	
}
