package com.boco.mbd.bean.demo;

/**
 * Student实体类，与数据库中STUDENT表对应
 * @author liangpeng
 * @date 2017-02-07
 */
public class Student {

	private int id;
	private String name;
	private int sex;
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getSex() {
		return sex;
	}
	public void setSex(int sex) {
		this.sex = sex;
	}
}
